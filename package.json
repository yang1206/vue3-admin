{
  "name": "vue-template",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "author": "Yang1206 <https://github.com/yang1206>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yang1206/vue-template.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:lint-staged": "lint-staged",
    "test": "vitest",
    "commit": "git pull && git add -A && git-cz && git push"
  },
  "dependencies": {
    "@tanstack/vue-query": "^4.27.0",
    "@unocss/reset": "0.51.5",
    "@vueuse/core": "^10.0.0",
    "axios": "^1.3.5",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.34",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.4",
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.4",
    "@iconify-json/carbon": "^1.1.16",
    "@iconify-json/line-md": "^1.1.24",
    "@types/node": "^18.15.11",
    "@types/nprogress": "^0.2.0",
    "@unocss/eslint-config": "^0.51.0",
    "@vitejs/plugin-vue": "4.1.0",
    "@vue/test-utils": "^2.3.2",
    "commitizen": "^4.3.0",
    "cz-git": "^1.6.1",
    "eslint": "^8.38.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.2.1",
    "typescript": "^5.0.4",
    "unocss": "0.51.5",
    "unplugin-auto-import": "0.15.3",
    "unplugin-vue-components": "^0.24.1",
    "vite": "4.3.0",
    "vitest": "^0.30.0",
    "vue-tsc": "1.2.0"
  },
  "lint-staged": {
    "*.{vue,js,ts}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "useEmoji": true
    }
  },
  "eslintConfig": {
    "extends": [
      "@antfu",
      "@unocss"
    ]
  }
}
