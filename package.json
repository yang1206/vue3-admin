    "vite-plugin-vue-devtools": "7.0.4",
    "vite-plugin-warmup": "^0.1.0",
    "vitest": "^1.1.1",
    "vue-tsc": "1.8.27"
  },
  "pnpm": {
    "allowedDeprecatedVersions": {
      "sourcemap-codec": "*",
      "w3c-hr-time": "*",
      "stable": "*"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "useEmoji": true
    }
  }
