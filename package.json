    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "qs": "^6.11.2",
    "vue": "^3.4.4",
    "vue-draggable-plus": "^0.3.4",
    "vue-router": "^4.2.5"
