{
  "name": "vue3-admin-template",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.7.5",
  "author": "Yang1206 <https://github.com/yang1206>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yang1206/vue3-admin.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:lint-staged": "lint-staged",
    "test": "vitest",
    "commit": "git pull && git add -A && git-cz && git push"
  },
  "dependencies": {
    "@tanstack/vue-query": "^4.35.3",
    "@vueuse/core": "^10.4.1",
    "axios": "^1.5.0",
    "colord": "^2.9.3",
    "date-fns": "^2.30.0",
    "lodash-es": "^4.17.21",
    "naive-ui": "^2.34.4",
    "naive-ui-pro-components": "0.3.0-beta.1",
    "pinia": "^2.1.6",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.42.0",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@iconify/json": "^2.2.115",
    "@iconify/vue": "^4.1.1",
    "@types/crypto-js": "^4.1.2",
    "@types/lodash-es": "^4.17.9",
    "@types/node": "^18.17.15",
    "@unocss/eslint-config": "^0.55.7",
    "@unocss/reset": "0.55.7",
    "@vitejs/plugin-vue": "4.3.4",
    "@vue-macros/volar": "^0.14.3",
    "@vue/runtime-core": "^3.3.4",
    "@vue/test-utils": "^2.4.1",
    "commitizen": "^4.3.0",
    "crypto-js": "^4.1.1",
    "cz-git": "^1.7.1",
    "eslint": "^8.49.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "mockjs": "^1.1.0",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.67.0",
    "sharp": "^0.32.5",
    "svgo": "^3.0.2",
    "typescript": "^5.2.2",
    "unocss": "0.55.7",
    "unplugin-auto-import": "0.16.6",
    "unplugin-icons": "^0.17.0",
    "unplugin-vue-components": "^0.25.2",
    "unplugin-vue-macros": "^2.5.1",
    "vite": "^4.4.9",
    "vite-plugin-compression2": "^0.10.4",
    "vite-plugin-image-optimizer": "^1.1.7",
    "vite-plugin-mkcert": "^1.16.0",
    "vite-plugin-mock": "2.9.8",
    "vite-plugin-pwa": "^0.16.5",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-devtools": "^1.0.0-rc.4",
    "vitest": "^0.34.4",
    "vue-tsc": "1.8.11"
  },
  "pnpm": {
    "allowedDeprecatedVersions": {
      "sourcemap-codec": "*",
      "w3c-hr-time": "*",
      "stable": "*"
    }
  },
  "lint-staged": {
    "*.{vue,js,ts}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "useEmoji": true
    }
  },
  "eslintConfig": {
    "extends": [
      "@antfu",
      "@unocss"
    ]
  }
}
