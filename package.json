{
  "name": "vue3-admin-template",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.7.5",
  "author": "Yang1206 <https://github.com/yang1206>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yang1206/vue3-admin.git"
  },
  "engines": {
    "node": "20"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "postinstall": "npx simple-git-hooks",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:lint-staged": "lint-staged",
    "check:types": "vue-tsc --noEmit",
    "test": "vitest",
    "commit": "git pull && git add -A && git-cz && git push"
  },
  "dependencies": {
    "@tanstack/vue-query": "^5.8.7",
    "@vueuse/core": "^10.6.1",
    "axios": "^1.6.2",
    "browser-update": "^3.3.44",
    "change-case": "^5.2.0",
    "colord": "^2.9.3",
    "dayjs": "^1.11.10",
    "lodash-es": "^4.17.21",
    "naive-ui": "^2.35.0",
    "naive-ui-pro-components": "0.3.0-beta.1",
    "pinia": "^2.1.7",
    "qs": "^6.11.2",
    "vue": "^3.3.9",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.1.1",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@iconify/json": "^2.2.147",
    "@iconify/vue": "^4.1.1",
    "@types/browser-update": "^3.3.3",
    "@types/crypto-js": "^4.2.1",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.10.0",
    "@types/qs": "^6.9.10",
    "@unocss/eslint-config": "^0.57.7",
    "@unocss/reset": "0.57.7",
    "@vitejs/plugin-vue": "4.5.0",
    "@vue-macros/volar": "^0.17.4",
    "@vue/runtime-core": "^3.3.9",
    "@vue/test-utils": "^2.4.2",
    "commitizen": "^4.3.0",
    "crypto-js": "^4.2.0",
    "cz-git": "^1.7.1",
    "eslint": "^8.54.0",
    "jsdom": "^23.0.0",
    "lint-staged": "^15.1.0",
    "mockjs": "^1.1.0",
    "postcss-html": "^1.5.0",
    "rollup-plugin-visualizer": "^5.9.3",
    "sass": "^1.69.5",
    "sharp": "^0.32.6",
    "simple-git-hooks": "^2.9.0",
    "stylelint": "^15.11.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^4.4.0",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^11.1.0",
    "stylelint-order": "^6.0.3",
    "svgo": "^3.0.4",
    "typescript": "^5.3.2",
    "unocss": "0.57.7",
    "unplugin-auto-import": "0.17.1",
    "unplugin-icons": "^0.17.4",
    "unplugin-vue-components": "^0.25.2",
    "unplugin-vue-macros": "^2.7.0",
    "vite": "^5.0.2",
    "vite-plugin-compression2": "^0.11.0",
    "vite-plugin-image-optimizer": "^1.1.7",
    "vite-plugin-mkcert": "^1.17.1",
    "vite-plugin-mock": "2.9.8",
    "vite-plugin-pwa": "^0.17.2",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-devtools": "1.0.0-rc.5",
    "vitest": "^0.34.6",
    "vue-tsc": "1.8.22"
  },
  "pnpm": {
    "allowedDeprecatedVersions": {
      "sourcemap-codec": "*",
      "w3c-hr-time": "*",
      "stable": "*"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*.{vue,js,ts}": "eslint --fix",
    "*.{scss,css,vue}": "stylelint  --fix --allow-empty-input",
    "*.{ts,cts,mts,tsx,vue}": "vue-tsc --noEmit"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "useEmoji": true
    }
  }
}
