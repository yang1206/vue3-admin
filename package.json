{
  "name": "vue3-admin-template",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.1.0",
  "author": "Yang1206 <https://github.com/yang1206>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yang1206/vue3-admin.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:lint-staged": "lint-staged",
    "test": "vitest",
    "commit": "git pull && git add -A && git-cz && git push"
  },
  "dependencies": {
    "@tanstack/vue-query": "^4.29.7",
    "@vueuse/core": "^10.1.2",
    "axios": "^1.4.0",
    "colord": "^2.9.3",
    "date-fns": "^2.30.0",
    "lodash-es": "^4.17.21",
    "naive-ui": "^2.34.4",
    "pinia": "^2.1.3",
    "vue": "^3.3.4",
    "vue-router": "^4.2.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.1",
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@iconify/json": "^2.2.67",
    "@iconify/vue": "^4.1.1",
    "@types/crypto-js": "^4.1.1",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^18.16.5",
    "@unocss/eslint-config": "^0.52.0",
    "@unocss/reset": "0.52.0",
    "@vitejs/plugin-vue": "4.2.3",
    "@vue/runtime-core": "^3.3.4",
    "@vue/test-utils": "^2.3.2",
    "commitizen": "^4.3.0",
    "crypto-js": "^4.1.1",
    "cz-git": "^1.6.1",
    "eslint": "^8.39.0",
    "husky": "^8.0.3",
    "jsdom": "^22.0.0",
    "lint-staged": "^13.2.2",
    "mockjs": "^1.1.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.62.1",
    "typescript": "^5.0.4",
    "unocss": "0.52.0",
    "unplugin-auto-import": "0.16.1",
    "unplugin-icons": "^0.16.1",
    "unplugin-vue-components": "^0.24.1",
    "vite": "4.3.8",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-mkcert": "^1.15.0",
    "vite-plugin-mock": "^2.9.8",
    "vite-plugin-pwa": "^0.15.0",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-devtools": "^0.0.16",
    "vitest": "^0.31.1",
    "vue-tsc": "1.6.5"
  },
  "lint-staged": {
    "*.{vue,js,ts}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "useEmoji": true
    }
  },
  "eslintConfig": {
    "extends": [
      "@antfu",
      "@unocss"
    ]
  }
}
