{
  "name": "vue3-admin-template",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "author": "Yang1206 <https://github.com/yang1206>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yang1206/vue3-admin.git"
  },
  "engines": {
    "node": "20"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "postinstall": "npx simple-git-hooks",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:lint-staged": "lint-staged",
    "lint:style": "stylelint **/*.{vue,css,scss} --fix",
    "check:types": "vue-tsc --noEmit",
    "test": "vitest",
    "commit": "git pull && git add -A && git-cz && git push"
  },
  "dependencies": {
    "@tanstack/vue-query": "^5.62.0",
    "@vueuse/core": "^12.0.0",
    "axios": "^1.7.8",
    "browser-update": "^3.3.55",
    "change-case": "^5.4.4",
    "colord": "^2.9.3",
    "dayjs": "^1.11.13",
    "lodash-es": "^4.17.21",
    "naive-ui": "^2.40.2",
    "naive-ui-pro-components": "0.3.1",
    "pinia": "^2.2.8",
    "pinia-plugin-persistedstate": "^4.1.3",
    "qs": "^6.13.1",
    "vue": "^3.5.13",
    "vue-draggable-plus": "^0.6.0",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.11.2",
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@iconify/json": "^2.2.278",
    "@iconify/vue": "^4.1.2",
    "@plugin-web-update-notification/vite": "^1.7.1",
    "@types/browser-update": "^3.3.3",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.17.9",
    "@types/qs": "^6.9.17",
    "@unocss/eslint-config": "^0.64.1",
    "@unocss/reset": "0.64.1",
    "@vitejs/plugin-legacy": "^6.0.0",
    "@vitejs/plugin-vue": "5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vue/runtime-core": "^3.5.13",
    "@vue/test-utils": "^2.4.6",
    "commitizen": "^4.3.1",
    "crypto-js": "^4.2.0",
    "cz-git": "^1.11.0",
    "eslint": "^9.16.0",
    "eslint-plugin-format": "^0.1.3",
    "jsdom": "^25.0.1",
    "lint-staged": "^15.2.10",
    "mockjs": "^1.1.0",
    "postcss-html": "^1.7.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.81.0",
    "sharp": "^0.33.5",
    "simple-git-hooks": "^2.11.1",
    "stylelint": "^16.11.0",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-recess-order": "^5.1.1",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-order": "^6.0.4",
    "svgo": "^3.3.2",
    "typescript": "5.6.3",
    "unocss": "0.64.1",
    "unplugin-auto-import": "0.18.6",
    "unplugin-icons": "^0.20.2",
    "unplugin-vue-components": "^0.27.5",
    "unplugin-vue-macros": "^2.13.4",
    "vite": "^6.0.1",
    "vite-plugin-checker": "^0.8.0",
    "vite-plugin-compression2": "^1.3.3",
    "vite-plugin-image-optimizer": "^1.1.8",
    "vite-plugin-inspect": "^0.10.2",
    "vite-plugin-mkcert": "^1.17.6",
    "vite-plugin-mock": "2.9.8",
    "vite-plugin-pwa": "^0.21.1",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-devtools": "7.6.7",
    "vite-plugin-warmup": "^0.1.0",
    "vitest": "^2.1.6",
    "vue-tsc": "2.1.10"
  },
  "pnpm": {
    "allowedDeprecatedVersions": {
      "sourcemap-codec": "*",
      "w3c-hr-time": "*",
      "stable": "*"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "useEmoji": true
    }
  }
}
